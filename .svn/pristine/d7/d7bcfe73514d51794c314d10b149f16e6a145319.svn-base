.tAlready{
  ul, ol {
    margin: 0;
  }
  .tAlreadyBox{
    border-bottom: 0.15rem solid #DEEAE1;
  }
  .tAlreadyHeader{
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    align-items: center;
    padding: 0.18rem 0.15rem;
    border-bottom: 0.01rem solid #DBDBDB;
  }
  .HeaderLeft{
    >div{
      color:#222222;
      font-size: 0.17rem;
      margin-bottom: 0.05rem;
    }
    >p{
      color:rgba(153,153,153,1);
      font-size: 0.12rem;
    }
  }
  .HeaderRight{
    >div{
      color:rgba(26,173,25,1);
      font-size: 0.19rem;
      font-weight: bold;
    }
    >p{
      text-align: center;
      color:#222;
      font-size: 0.13rem;
    }
  }
  .btns {
    width: 100%;
    display: flex;
    justify-content: center;
    align-content: center;
    box-sizing: border-box;
    padding: 0.08rem 0;
    button {
      border: 0;
      color:rgba(26,173,25,1);
      background: #FFFFFF;
      font-size: 0.14rem;
    }
    .vux-x-icon {
      fill: #DBDBDB;
    }
  }
}
