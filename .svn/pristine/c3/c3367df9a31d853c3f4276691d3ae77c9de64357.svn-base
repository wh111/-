.tNone{
  .tAlreadyBox{
    border-bottom: 0.15rem solid #DEEAE1;
  }
  .tAlreadyHeader{
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    align-items: center;
    padding: 0.18rem 0.15rem;
    border-bottom: 0.01rem solid #DBDBDB;
  }
  .HeaderLeft{
    >div{
      color:rgba(34,34,34,0.8);
      font-weight: bold;
      font-size: 0.17rem;
      margin-bottom: 0.05rem;
    }
    >p{
      color:rgba(153,153,153,1);
      font-size: 0.12rem;
    }
  }
  .HeaderRight{
    button{
      width: 0.55rem;
      height: 0.22rem;
      line-height: 0.22rem;
      padding: 0;
      font-size: 0.11rem;
      color:rgba(255,255,255,0.9);
    }
  }
  .harryUp{
    padding:0 0.15rem;
    .item{
      height: 0.44rem;
      color: black;
      line-height: 0.44rem;
      font-size: 0.17rem;
      border-bottom: 0.01rem solid #E5E5E5;
      button{
        float: right;
        margin-top: 0.05rem;
      }
    }
  }
}
