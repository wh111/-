.tMine{
  .Tabs{
    color: #000;
    font-size: 0.15rem;
  }
  .hello_img{
    position: relative;
    width: 100%;
    text-align: center;
    >img{
      margin-top: 0.23rem;
    }
    >div{
      font-weight: bold;
      color: #808080;
      margin-bottom: 0.06rem;
    }
    .firstChild_news{
      width: 0.3rem;
      height: 0.3rem;
      position: absolute;
      top: 0;
      right: 0.23rem;
    }
  }
  .hello_dynamic{
    border-bottom: 0.05rem solid #DEEAE1;
    border-top: 0.05rem solid #DEEAE1;
    box-sizing: border-box;
    padding: 0.2rem 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
    >div{
      width: 50%;
      >div{
        color: #434343;
        font-size: 0.12rem;
      }
      text-align: center;
      font-size: 0.12rem;
      >span{
        color: #11A640;
        font-size: 0.12rem;
        font-weight: bold;
      }
    }
    >div:not(:last-of-type){
      border-right: 0.02rem solid #F3F3F3;
    }
  }
  .hello_tab{
    box-sizing: border-box;
    padding: 0 0.15rem 0.15rem 0.15rem;
    >div{
      font-weight: bolder;
      color: #454545;
      display: flex;
      justify-content: space-between;
      box-sizing: border-box;
      padding: 0.15rem 0;
      border-bottom: 0.01rem solid #F0F6F2;
    }
  }
  .hello_tab{
    >div{
      display: flex;
      align-items: center;
    }
  }
  .vux-x-icon {
    fill: #DBDBDB;
  }
}
