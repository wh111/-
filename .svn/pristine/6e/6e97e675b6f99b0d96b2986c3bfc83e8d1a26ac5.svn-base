*{
  margin: 0;
  padding: 0;
  list-style: none;
}
.recentPractice_content{
  .contents{
    display: flex;
    align-items: center;
    border-bottom: 0.02rem solid #E9E9E9;
    box-sizing: border-box;
    padding-right: 0.15rem;
    padding-left: 0.1rem;
    >:first-child{
      border: 1px solid #E9E9E9;
      border-radius: 0.05rem;
      width: 0.9rem;
      display: flex;
      justify-content: center;
      align-items: center;
      padding: 0.08rem 0.2rem;
      margin: 0.2rem 0.15rem;
      >img{
        width: 0.675rem;
        height: 0.9rem;
      }
    }
  }
  .homePage_book{
    >div{
      font-size: 0.16rem;
      color: #5D5D5D;
      font-weight: bolder;
      margin-bottom: 0.03rem;
    }
    >div:not(:first-child){
      display: inline-block;
      margin-right: 0.2rem;
      margin-bottom: 0.03rem;
      span{
        font-size: 0.12rem;
      }
      >span:nth-of-type(1){
        color: #909090;
        margin-right: 0.05rem;
      }
      >span:nth-of-type(2){
        color: #EF7E5D;
      }
    }
    p{
      font-size: 0.14rem;
      color: #909090;
    }
  }
  .weui-progress__bar{
    width: 1rem;
  }
  .recentContent{
    box-sizing: border-box;
    padding: 0.17rem 0.15rem;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 0.01rem solid #F6F9F7;
    font-weight: bold;
    >li>div>p{
      color: #5F5F5F;
      font-size: 0.17rem;
      margin-bottom: 0.1rem;
    }
    .one{
      font-size: 0.12rem;
      color: #A4A4A4;
      margin-right: 0.05rem;
    }
    .two{
      font-size: 0.12rem;
      color: #F19276;
      margin-right: 0.1rem;
    }
    li:nth-of-type(2){
      color: #A4A4A4;
      font-size: 0.3rem;
    }
  }
  .progress1{
    margin: 0.08rem 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 2rem;
    >div{
      color: #A4A4A4;
      font-size: 0.12rem;
    }
  }
  div.weui-progress{
    width: 70%;
  }
  .content{
    border-bottom: 0.05rem solid #DEEAE1;
    padding-bottom: 0.3rem
  }
  .vux-x-icon {
    fill: #DBDBDB;
  }
  #progress1_x{
    border-radius: 0.5rem;
    overflow: hidden;
  }
  #progress1_x > div{
    height: 10px;
  }

}
