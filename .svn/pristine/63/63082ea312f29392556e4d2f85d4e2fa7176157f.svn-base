.tQuestion{
  .tQuestionHeader{
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 0.08rem 0.15rem;
    border-bottom: 1px solid #F1F1F1;
    >li:nth-of-type(2){
      color:rgba(178,178,178,1);
      font-size: 0.17rem;
    }
  }
  .tQuestionLeft{
    >div{
      font-weight: bold;
      color:rgba(0,0,0,0.7);
      font-size: 0.17rem;
      margin-bottom: 0.05rem;
    }
    p{
      color:rgba(136,136,136,1);
      font-size: 0.13rem;
    }
  }
}
