.mistakes{
  .mistakes_content{
    .mistakes_content_header{
      background: #F1F5F4;
      box-sizing: border-box;
      padding: 0.15rem 0.15rem 0.08rem 0.15rem;
      font-size: 0.14rem;
      color:rgba(136,136,136,0.9);
      font-weight: bold;
    }
    .mistakes_content_topic{
      box-sizing: border-box;
      >li:nth-of-type(1){
        border-bottom: 1px solid #F0F0F0;
        box-sizing: border-box;
        padding:0.15rem;
        overflow: hidden;
        height: 1.2rem;
        overflow: hidden;
        color:rgba(0,0,0,0.7);
        font-weight: bold;
        >div:nth-of-type(1){
          margin-bottom: 0.1rem;
        }
      }
      >li:nth-of-type(2){
        display: flex;
        justify-content: space-between;
        align-items: center;
        box-sizing: border-box;
        padding: 0 0.15rem;
        height: 0.53rem;
        line-height: 0.43rem;
        border-bottom: 0.1rem solid #F1F5F4;
        >div:nth-of-type(1){
          color:rgba(0,0,0,0.7);
          font-weight: bold;
        }
        .vux-x-icon {
          fill: #DBDBDB;
        }
      }
    }
  }
}
