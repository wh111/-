.tClassCreated{
  .tName{
    box-sizing: border-box;
    padding-bottom: 0.2rem;
    border-bottom: 0.01rem solid #EBEBEB;
    >p{
      box-sizing: border-box;
      padding: 0.15rem;
      color:rgba(34,34,34,0.8);
      font-weight: bold;
      font-size: 0.17rem;
    }
    >div{
      text-align: center;
      >input{
        width: 3.45rem;
        height: 0.285rem;
        outline: 0;
        border: 0;
        box-sizing: border-box;
        padding: 0.15rem 0;
        background: #EBEBEB;
        text-indent:0.06rem;
        border-radius: 0.05rem;
      }
    }
  }
  .tClassSubmit{
    background: #DEEAE1;
    box-sizing: border-box;
    padding-top: 0.3rem;
    height: 4.11rem;
    width: 100%;
    .weui-btn{
      width: 3.35rem;
      height: 0.47rem;
      margin: 0 auto;
    }

  }
}
