.recentPractice_next{
  .tab_click{
    display: flex;
    justify-content: center;
    >div{
      display: flex;
      justify-content: center;
      align-items: center;
      box-sizing: border-box;
      padding: 0.13rem 0.15rem;
      width: 0.65rem;
      height: 0.25rem;
      font-size: 0.13rem;
      border-radius: 0.05rem;
      margin: 0.1rem 0.1rem 0.1rem 0;
    }
    >div:nth-of-type(1){
      color: #91B1DD;
      border: 0.02rem solid #91B1DD;
    }
    >div:nth-of-type(2){
      color: #37C0BD;
      border: 0.02rem solid #37C0BD;
    }
    >div:nth-of-type(3){
      color: #F5AE79;
      border: 0.02rem solid #F5AE79;
    }
    >:last-child{
      margin-right: 0;
    }
  }
  .txt{
    width: 1.5rem;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
  }
}
