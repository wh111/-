.questionBank{
  background: #fff;
  .questionBankHeader{
    position: fixed;
    background: #FFFFFF;
    width: 100%;
  }
  .position{
    position: inherit;
  }
  .select{
    width: 90%;
    background: white;
    border-radius: 0.02rem;
  }
  .questionBank_nav{
    display: flex;
    justify-content: space-around;
    box-sizing: border-box;
    padding: 0.075rem 0;
    width: 100%;
    text-align: center;
    border-bottom: 1px solid #E8EAEC;

    select{
      -webkit-appearance: none;
      -webkit-tap-highlight-color: #fff;
      outline: 0;
      border: none;
      color: #11A640;
      //border: 0.01rem solid #11A640;
      text-align: center;
      width: auto;
      //min-width: 0.8rem;
      padding: 0 2%;
      margin: 0;
    }
    option{

      text-align:center;

    }
